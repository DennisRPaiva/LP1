#include <stdio.h>
#include <math.h>


int todos_os_primos(long long int prim)
{
   int i,j;
   int cont = 2;
  for (i=1;i<=prim;i++)
   {
      if(eh_primo(i))
      {
         continue;
      }
      cont++;
   }
   return cont;
}

int eh_primo(long long int primo)
{
   int i;
   for (i=2;i<=sqrt(primo);i++)
   {
      if(primo % i == 0)
      {
         return 1; // Nao eh primo
      }
   }
   return 0; // Eh primo
}

int main()
{
   long long int num;
   int x;
   printf("Escreva um numero: ");
   scanf("%lld",&num);
   
   
}
